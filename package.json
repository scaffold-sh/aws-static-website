  "types": "./src/main.ts",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Scaffold",
    "email": "support@scaffold.sh",
    "url": "https://scaffold.sh"
  },
  "homepage": "https://scaffold.sh/docs/infrastructures/aws/static-website",
  "repository": {
    "type": "git",
    "url": "https://github.com/scaffold-sh/aws-static-website.git"
  },
  "bugs": {
